{
  "name": "blog-admin",
  "version": "0.1.0",
  "main": "index.js",
  "scripts": {
    "admin": "parallelshell \"webpack --config webpack.admin.js --watch\" \"http-server -e html -p 5000 -c-1\"",
    "pretokenUpdate": "cp ./lambdas/returnToken.js ./",
    "tokenUpdate": "deployless update bucket=ams-admin name=returnToken key=returnToken.zip file=returnToken.js",
    "posttokenUpdate": "rm returnToken.js",
    "preloginUpdate": "cp ./lambdas/handleLogin.js ./",
    "loginUpdate": "deployless update bucket=ams-admin name=handleLogin key=handleLogin.zip file=handleLogin.js",
    "postloginUpdate": "rm handleLogin.js",
    "preupdateBuildIndex": "cp ./lambdas/buildIndexPage.js ./",
    "updateBuildIndex": "deployless update bucket=ams-admin name=buildIndexPage key=buildIndexPage.zip file=buildIndexPage.js",
    "postupdateBuildIndex": "rm buildIndexPage.js",
    "test": "http-server ./site -p 9000 -e html",
    "fix": "eslint --fix ./lambdas/*.js"
  },
  "license": "MIT",
  "dependencies": {
    "aws-sdk": "^2.6.11",
    "babel-cli": "^6.22.2",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "deployless": "^0.1.0",
    "es6-promise": "^4.0.5",
    "eslint": "^3.15.0",
    "eslint-config-sparkbox": "^0.4.0",
    "fetch-ponyfill": "^3.0.2",
    "he": "^1.1.0",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.4",
    "moment": "^2.17.1",
    "mysql": "^2.12.0",
    "parallelshell": "^2.0.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "sendgrid": "^4.7.0",
    "sqlite3": "^3.1.8",
    "webpack": "^2.2.1"
  },
  "devDependencies": {
    "http-server": "^0.9.0"
  }
}
