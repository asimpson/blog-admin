{
  "name": "blog-admin",
  "version": "0.2.0",
  "scripts": {
    "admin": "node admin.js",
    "preupdate": "npm run webpack:lambda && npm run browser && npm run postcss",
    "update": "node publish.js",
    "search": "node search.js",
    "webpack:lambda": "webpack --config webpack.lambda.js",
    "nodemon": "nodemon server.js",
    "postcss": "postcss css/base.css -o packages/node_modules/site.css",
    "postcss:watch": "postcss -w css/base.css -o packages/node_modules/site.css",
    "start": "run-p nodemon postcss:watch",
    "browser": "NODE_ENV=production webpack --config webpack.browser.js -p"
  },
  "dependencies": {
    "aws-sdk": "^2.6.11",
    "babel-cli": "^6.22.2",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "consolidate": "^0.14.5",
    "content-type-mime": "^1.1.0",
    "deployless": "^0.3.1",
    "es6-promise": "^4.0.5",
    "eslint": "^3.15.0",
    "eslint-config-sparkbox": "^0.4.0",
    "fetch-ponyfill": "^3.0.2",
    "handlebars": "^4.0.6",
    "he": "^1.1.0",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.4",
    "lunr": "^2.0.4",
    "marked": "^0.3.6",
    "moment": "^2.17.1",
    "mysql": "^2.12.0",
    "parallelshell": "^2.0.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "sendgrid": "^4.7.0",
    "sqlite3": "^3.1.8",
    "to-markdown": "^3.0.3",
    "webpack": "^2.2.1"
  },
  "devDependencies": {
    "cssnano": "^3.10.0",
    "express": "^4.15.3",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.2",
    "postcss": "^6.0.8",
    "postcss-cli": "^4.1.0",
    "postcss-nested": "^2.0.4",
    "postcss-nesting": "^4.0.1",
    "postcss-partial-import": "^4.1.0",
    "postcss-simple-vars": "^4.1.0"
  }
}
