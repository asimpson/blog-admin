{
  "name": "blog-admin",
  "version": "0.1.0",
  "main": "index.js",
  "scripts": {
    "admin": "parallelshell \"webpack --config webpack.admin.js --watch\" \"http-server -e html -p 5000 -c-1\"",
    "search": "parallelshell \"webpack --config webpack.search.js --watch\" \"http-server -e html -p 5000 -c-1\"",
    "pretokenUpdate": "cp ./lambdas/returnToken.js ./",
    "tokenUpdate": "deployless update bucket=ams-admin name=returnToken key=returnToken.zip file=returnToken.js",
    "posttokenUpdate": "rm returnToken.js",
    "preloginUpdate": "cp ./lambdas/handleLogin.js ./",
    "loginUpdate": "deployless update bucket=ams-admin name=handleLogin key=handleLogin.zip file=handleLogin.js",
    "postloginUpdate": "rm handleLogin.js",
    "preupdateBuildIndex": "cp ./lambdas/buildIndexPages.js ./ && cp ./lambdas/template.js ./",
    "updateBuildIndex": "deployless update bucket=ams-admin name=buildIndexPages app.js template.js rss.hbs",
    "postupdateBuildIndex": "rm buildIndexPages.js template.js",
    "prebuildPosts": "cp ./lambdas/buildPosts.js ./ && cp ./lambdas/template.js ./",
    "buildPosts": "deployless update bucket=ams-admin name=buildPosts app.js template.js",
    "postbuildPosts": "rm buildPosts.js template.js",
    "prenewPost": "cp ./lambdas/newPost.js ./",
    "newPost": "deployless update bucket=ams-admin name=newPost",
    "postnewPost": "rm newPost.js",
    "preupdatePost": "cp ./lambdas/updatePost.js ./",
    "updatePost": "deployless update bucket=ams-admin name=updatePost",
    "postupdatePost": "rm updatePost.js",
    "prereturnPosts": "cp ./lambdas/returnPosts.js ./",
    "returnPosts": "deployless update bucket=ams-admin name=returnPosts",
    "postreturnPosts": "rm returnPosts.js",
    "precache": "cp ./lambdas/invalidate.js ./",
    "cache": "deployless update bucket=ams-admin name=invalidate",
    "postcache": "rm invalidate.js",
    "test": "parallelshell 'http-server ./ -p 9000 -e html' 'webpack --config webpack.admin.js -w'",
    "buildAdmin": "webpack --config webpack.admin.js",
    "deploy": "node deploy.js",
    "prod": "webpack -p --config webpack.admin.js",
    "predeploySearch": "cp ./lambdas/buildSearch.js .",
    "deploySearch": "deployless update bucket=ams-admin name=buildSearch",
    "postdeploySearch": "rm ./buildSearch.js",
    "fix": "eslint --fix ./lambdas/*.js"
  },
  "license": "MIT",
  "dependencies": {
    "aws-sdk": "^2.6.11",
    "babel-cli": "^6.22.2",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "consolidate": "^0.14.5",
    "content-type-mime": "^1.1.0",
    "deployless": "^0.2.1",
    "es6-promise": "^4.0.5",
    "eslint": "^3.15.0",
    "eslint-config-sparkbox": "^0.4.0",
    "fetch-ponyfill": "^3.0.2",
    "handlebars": "^4.0.6",
    "he": "^1.1.0",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.4",
    "lunr": "^2.0.4",
    "marked": "^0.3.6",
    "moment": "^2.17.1",
    "mysql": "^2.12.0",
    "parallelshell": "^2.0.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "sendgrid": "^4.7.0",
    "sqlite3": "^3.1.8",
    "to-markdown": "^3.0.3",
    "webpack": "^2.2.1"
  },
  "devDependencies": {
    "http-server": "^0.9.0"
  }
}
